

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <div class="panel-body">
                <div id="calendar"></div>
            </div>
        </section>
    </div>
</div>
    <script type='text/javascript' src='jquery-1.8.1.min.js'></script>
    <script>
    $('#calendar').fullCalendar({
       header: {
                     left: 'prev,next today',
                     center: 'title',
                     right: 'month,agendaWeek,agendaDay'
                 },
                events: '/events.json'
            });

    </script>

<%= link_to 'New Event', new_event_path %>
<% Event.all.each do |x| %>
<%= x.title %>
<% end %>